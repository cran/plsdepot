Package: plsdepot
Type: Package
Title: Partial Least Squares (PLS) Data Analysis Methods
Version: 0.2.0
Date: 2023-03-12
Authors@R: c(
  person(given = "Frederic", family= "Bertrand", role = c("cre"), email = "frederic.bertrand@utt.fr", comment = c(ORCID = "0000-0002-0837-8281")),
